<% if(typeof errors != 'undefined') { %>
<div class="error-msg">
    <ul>
        <% errors.forEach(function(error) {%>
        <li>
            <%= error.msg%>
        </li>
        <% }); %>
    </ul>
</div>
<% } %>

<% if(success_msg != '') { %>
<div class="success-msg">
    <%= success_msg %>
</div>
<% } %>

<% if(error_msg != '') { %>
<div class="error-msg">
    <%= error_msg %>
</div>
<% } %>